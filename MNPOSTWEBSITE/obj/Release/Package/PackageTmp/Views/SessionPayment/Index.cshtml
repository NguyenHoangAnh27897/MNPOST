
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAccount.cshtml";
}

<section id="main-content">
    <section class="wrapper">
        <h3><i class="fa fa-angle-right"></i> Danh sách phiên chuyển tiền</h3>
        <div class="row">

            <div class="row mt">
                <div class="col-md-12">
                    <div class="content-panel">
                        <table class="table table-bordered table-striped table-condensed">
                            <h4><i class="fa fa-angle-right"></i> Phiên chuyển tiền</h4>
                            <hr />
                            <form method="get" action="/sessionpayment/SearchCOD">
                                <button class="btn btn-success" style="margin-right: 10px;margin-top: 19px;">Tra</button>
                                <div class='col-sm-3'>
                                    <div class="form-group">
                                        Từ ngày
                                        <div class='input-group date' id='dtpfromdate'>
                                            <input type='date' class="form-control" id="fdate" name="FromDate" />
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    Đến ngày
                                    <div class='input-group date' id='dtptodate'>
                                        <input type='date' class="form-control" name="ToDate" id="tdate" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                            </form>
                            <hr />
                            <thead>
                                <tr>
                                    <th><i class="fa fa-code"></i> Mã phiên</th>
                                    <th><i class="fa fa-code"></i> Mã bưu cục</th>
                                    <th><i class="fa fa-calendar"></i> Ngày tạo tài liệu</th>
                                    <th><i class="fa fa-code"></i> Số phiếu</th>
                                    <th><i class="fa fa-money"></i> Tổng cộng</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div><!-- /content-panel -->
                </div><!-- /col-md-12 -->
            </div><!-- /row -->
        </div>
    </section><! --/wrapper -->
</section><!-- /MAIN CONTENT -->
